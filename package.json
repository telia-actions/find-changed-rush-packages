{
  "name": "find-changed-rush-packages",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "postbuild": "cp package.json dist/package.json && cd dist && npm i --only=production",
    "format": "prettier . --check",
    "format:fix": "prettier . --check --write",
    "lint": "eslint **/*.ts --cache",
    "lint:fix": "eslint **/*.ts --fix --cache",
    "test": "jest --runInBand"
  },
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/exec": "^1.0.4",
    "@actions/github": "^5.0.0",
    "@microsoft/rush-lib": "^5.66.0",
    "@rushstack/node-core-library": "^3.43.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.9",
    "@typescript-eslint/parser": "^4.8.1",
    "@vercel/ncc": "^0.25.1",
    "eslint": "^7.17.0",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-jest": "^24.1.3",
    "jest": "^26.6.3",
    "jest-preset-typescript": "^1.2.0",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "typescript": "~4.1.3"
  }
}
